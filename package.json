{
  "name": "v-money",
  "version": "0.8.1",
  "description": "Vue currency input/directive mask",
  "main": "dist/v-money.js",
  "scripts": {
    "size": "size-limit",
    "upgrade:check": "ncu",
    "upgrade:apply": "ncu -u && npm install",
    "build": "vue-cli-service build --lib",
    "test": "jest",
    "lint": "vue-cli-service lint",
    "dev": "vue-cli-service serve --open",
    "build_old": "vue build ./src/index.js --config ./build.config.js --dist ./dist/ --prod --lib",
    "dev_old": "vue build ./src/docs/docs.vue --config ./src/docs/build.config.js --host 0.0.0.0",
    "docs:build": "vue build ./src/docs/docs.vue --config ./src/docs/build.config.js --dist ./docs/ --prod",
    "docs:serve": "open http://localhost:5000/docs/ && serve ./",
    "compile": "npm run build -- --disable-compress"
  },
  "keywords": [
    "vue",
    "input",
    "mask",
    "directive",
    "currency",
    "money"
  ],
  "files": [
    "dist/"
  ],
  "size-limit": [
    {
      "path": "dist/v-money.js",
      "limit": "1.6 KB"
    }
  ],
  "author": "Marcos Neves <marcos.neves@gmail.com> (https://vuejs-tips.github.io/)",
  "license": "MIT",
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.0.2",
    "@vue/babel-preset-app": "^4.2.2",
    "@vue/cli-plugin-eslint": "^4.2.2",
    "@vue/cli-service": "^4.2.2",
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^25.1.0",
    "eslint": "^6.8.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-vue": "^6.2.1",
    "jest": "^25.1.0",
    "npm-check-updates": "^4.0.1",
    "prettier": "^1.19.1",
    "size-limit": "^4.0.2",
    "vue-template-compiler": "^2.6.11",
    "vuetify": "^2.2.14"
  },
  "dependencies": {
    "vue": "^2.6.11"
  }
}
